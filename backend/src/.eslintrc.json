{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    // "prettier/prettier": "error",
    "spaced-comment": "off",
    "no-console": "error",
    "consistent-return": "off",
    "func-names": "off",
    "comma-spacing": "error",
    "semi": "error",
    "semi-spacing": "error",
    "no-else-return": "error",
    "no-unused-labels": "error"
  }
}
