{
  "compilerOptions": {
    "types": ["node", "express"],
    "allowJs": false,
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./setupServer",
    // "rootDirs": ["src", "controllers", "routes"],
    "declaration": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["setupServer/**/*"]
}
